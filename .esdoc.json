{
  "source": "./lib",
  "destination": "./docs/",
  "includes": ["\\.js$"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "undocumentIdentifier": { "enable": false },
        "unexportedIdentifier": { "enable": true },
        "lint": { "enable": true },
        "coverage": { "enable": true },
        "typeInference": { "enable": true },
        "brand": {
          "title": "Response Handler",
          "description": "Configura el formato de la respuesta de un servicio web creado con express.",
          "repository": "https://github.com/insacjs/response-handler"
        }
      }
    }
  ]
}
